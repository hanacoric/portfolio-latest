te\" ref=\"website_img\" class=\"website_img\">\r\n      </div>\r\n    </section>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n        websiteImages: [\r\n        '../assets/images/website2.png',\r\n        '../assets/images/website3.png',\r\n        '../assets/images/website4.png',\r\n        '../assets/images/website5.png',\r\n        '../assets/images/website6.png',\r\n        '../assets/images/website.png'\r\n      ],\r\n      currentIndex: 0,\r\n      autoplayInterval: null,\r\n      autoplayDelay: 3000,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.startAutoplay();\r\n  },\r\n  methods: {\r\n    next() {\r\n      this.currentIndex = (this.currentIndex + 1) % this.websiteImages.length;\r\n    },\r\n    startAutoplay() {\r\n      this.autoplayInterval = setInterval(() => {\r\n        this.next();\r\n      }, this.autoplayDelay);\r\n    },\r\n    stopAutoplay() {\r\n      clearInterval(this.autoplayInterval);\r\n    },\r\n  },\r\n  beforeUnmount() {\r\n    this.stopAutoplay();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.white_section{\r\n    height: 200vh;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-evenly;\r\n    align-items: center;\r\n    background-color: white;\r\n    font-size: 2em;\r\n}\r\n\r\n.white_section h1{\r\n    font-family: belfastgroteskbold;\r\n}\r\n\r\n.projects {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content:space-between;\r\n    align-items: center;\r\n    color: black;\r\n    text-align: center;  \r\n    height: min-content;\r\n}\r\n\r\n.posters{\r\n    width: 80vw;\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: space-evenly;\r\n    align-items: center;\r\n    height: min-content;\r\n}\r\n\r\n.slideshow_posters{\r\n    width: 300px;\r\n    height: 398px;\r\n}\r\n\r\n.logos{\r\n    width: 80vw;\r\n    display: flex;\r\n    flex-direction: row-reverse;\r\n    justify-content: space-evenly;\r\n    align-items: